<table>
	<thead>
		<tr>
		   <th>Name</th>
		   <th>Balance</th>
		</tr>
	</thead>

	<tbody>
		{{#customers}}
		<tr>
		   <td>{{name.first}} {{name.last}}</td>
		   <td>{{balance}}</td>
		</tr>
		{{/customers}}
	</tbody>

	<tfoot>
		<tr>
		   <td>Sum</td>
		   <td>$180</td>
		</tr>
	</tfoot>
</table>